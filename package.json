{
  "name": "revelo-tasks-tracker",
  "displayName": "Revelo Tasks Tracker",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "revelo-tasks-tracker.startTracking",
        "title": "Start Tracking"
      },
      {
        "command": "revelo-tasks-tracker.clipboardCopy",
        "title": "Custom Copy"
      },
      {
        "command": "revelo-tasks-tracker.clipboardPaste",
        "title": "Custom Paste"
      }
    ],
    "keybindings": [
      {
        "command": "revelo-tasks-tracker.clipboardCopy",
        "key": "ctrl+c",
        "mac": "cmd+c",
        "when": "editorTextFocus"
      },
      {
        "command": "revelo-tasks-tracker.clipboardPaste",
        "key": "ctrl+v",
        "mac": "cmd+v",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8"
  }
}
